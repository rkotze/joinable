{
  "name": "joinable",
  "version": "1.0.0",
  "description": "Join strings with built in control flow. Because we always need to join strings logically.",
  "scripts": {
    "test": "mocha -r should --compilers js:babel-core/register src/*spec.js src/**/*spec.js",
    "test:w": "npm test -- --watch",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run test && npm run compile"
  },
   "repository": {
    "type": "git",
    "url": "https://github.com/rkotze/joinable"
  },
  "keywords": [
    "strings",
    "concat",
    "conditional",
    "join",
    "simple"
  ],
  "main": "lib/index.js",
  "license":{ 
    "type" : "MIT",
    "url" : "https://github.com/rkotze/joinable/blob/master/LICENSE"
  },
  "author": "Richard Kotze <richkotze@outlook.com>",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.7.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.5.0",
    "mocha": "^2.4.5",
    "should": "^8.3.0",
    "should-sinon": "0.0.5",
    "sinon": "^1.17.3"
  },
  "bugs": {
    "url": "https://github.com/rkotze/joinable/issues"
  }
}
